      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lorenzo.imbrogiano.haarmoniebonusapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f9f0ea"
      },
      "package": "com.lorenzo.imbrogiano.haarmoniebonusapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3d64b3ff-d0bd-462d-af00-d8c66332412b"
      }
    },
    "owner": "lorenzo.imbrogiano"
  }
