{
  "expo": {
    "name": "Haarmonie by Cynthia",
    "slug": "haarmonie-bonus-app",
    "version": "1.0.0",
    "scheme": "haarmonie-bonus-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#f9f0ea"
    },
